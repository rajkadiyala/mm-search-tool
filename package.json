{
  "name": "mm",
  "version": "1.0.0",
  "description": "Miracle Messages Volunteer Search Tool",
  "engines": {
    "node": "10.5.0",
    "npm": "6.1.0"
  },
  "main": "index.js",
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "deploy": "script/deploy",
    "lint": "eslint ./ --ext .jsx --ext .js --ignore-path .gitignore",
    "lint-sass": "node node_modules/sass-lint/bin/sass-lint.js -c .sasslintrc.json -q -v",
    "lint-fix": "npm run lint --fix",
    "precommit": "# npm run lint # un-comment to enable",
    "postinstall": "touch secrets.js",
    "seed": "node script/seed.js",
    "start": "node server",
    "start-dev": "NODE_ENV='development' npm run build-client-watch & npm run start-server",
    "start-server": "nodemon server -e html,js,scss --ignore dist --ignore client",
    "test": "NODE_ENV='test' mocha \"./server/tests/index.spec.js\" --require @babel/polyfill --require @babel/register"
  },
  "dependencies": {
    "@babel/core": "7.0.0-beta.51",
    "@babel/polyfill": "7.0.0-beta.51",
    "@babel/preset-env": "7.0.0-beta.51",
    "@babel/preset-react": "7.0.0-beta.51",
    "@babel/preset-stage-2": "7.0.0-beta.51",
    "@babel/register": "7.0.0-beta.51",
    "axios": "0.18.0",
    "babel-eslint": "8.2.5",
    "babel-loader": "8.0.0-beta.2",
    "body-parser": "1.18.3",
    "bulma": "0.7.1",
    "chai": "4.1.2",
    "compression": "1.7.2",
    "connect-session-sequelize": "5.2.1",
    "css-loader": "1.0.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.10.0",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "font-awesome": "4.7.0",
    "googleapis": "32.0.0",
    "history": "4.7.2",
    "hoek": "5.0.3",
    "husky": "0.14.3",
    "mocha": "5.2.0",
    "morgan": "1.9.0",
    "node-sass": "4.9.2",
    "nodemon": "1.17.5",
    "passport": "0.4.0",
    "pg": "7.4.3",
    "pg-hstore": "2.3.2",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-select": "1.2.1",
    "redux": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "sass-lint": "1.12.1",
    "sass-loader": "7.0.3",
    "sequelize": "4.38.0",
    "style-loader": "0.21.0",
    "supertest": "3.1.0",
    "webpack": "4.14.0",
    "webpack-cli": "3.0.8"
  }
}
